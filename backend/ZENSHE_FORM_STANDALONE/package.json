{
  "name": "zenshe-form-standalone",
  "version": "1.0.0",
  "description": "Self-contained Zenshe Steam Therapy Form with integrated backend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
